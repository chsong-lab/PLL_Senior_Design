                Frame2.destroy()
            elif noise == 3:
                Frame2.destroy()
            elif noise == 4:
                Frame2.destroy()
            elif noise == 5:
                Frame2.destroy()
            elif noise == 6:
                Frame2.destroy()
            elif noise == 7:
                Frame2.destroy()
            elif noise == 8:
                Frame2.destroy()
            else:
                Frame2.destroy()
                
